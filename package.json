{
  "name": "webpack-boilterplate",
  "version": "1.0.0",
  "description": "A good starting point for using webpack to build stuffs.",
  "author": "tomzhu1024",
  "license": "MIT",
  "repository": "https://github.com/tomzhu1024/webpack-boilerplate",
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack build",
    "analyze-bundle": "cross-env NODE_ENV=production ANALYZER=true webpack build",
    "type-check": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{css,sass,scss,less}\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.{css,sass,scss,less}\"",
    "prettier": "prettier --check \"(src|public)/**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\"",
    "prettier:fix": "prettier --write \"(src|public)/**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.14",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "querystring-es3": "^0.2.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
}
