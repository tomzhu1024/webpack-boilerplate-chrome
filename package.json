{
  "name": "webpack-boilterplate",
  "version": "1.0.0",
  "description": "A good starting point for using webpack to build stuffs.",
  "author": "tomzhu1024",
  "license": "MIT",
  "repository": "https://github.com/tomzhu1024/webpack-boilerplate",
  "private": true,
  "browserslist": [
    "> 0.5%",
    "last 2 version",
    "not dead"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "node utils/start.js",
    "build": "cross-env NODE_ENV=production webpack build",
    "analyze-bundle": "cross-env NODE_ENV=production ANALYZER=true webpack build",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "eslint": "eslint \"./**/*.{js,ts,jsx,tsx}\"",
    "eslint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint \"./**/*.{css,sass,scss,less}\"",
    "stylelint:fix": "stylelint --fix \"./**/*.{css,sass,scss,less}\"",
    "prettier": "prettier --check \"./**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\"",
    "prettier:fix": "prettier --write \"./**/*.{html,js,jsx,ts,tsx,css,sass,scss,less,json}\"",
    "commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@jest/globals": "^29.7.0",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@pmmmwh/react-refresh-webpack-plugin": "patch:@pmmmwh/react-refresh-webpack-plugin@npm%3A0.5.11#~/.yarn/patches/@pmmmwh-react-refresh-webpack-plugin-npm-0.5.11-661b68d368.patch",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@types/chrome": "^0.0.248",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "querystring-es3": "^0.2.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-prettier": "^4.0.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "webpackbar": "patch:webpackbar@npm%3A5.0.2#~/.yarn/patches/webpackbar-npm-5.0.2-70d85f1a62.patch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.0.2"
}
